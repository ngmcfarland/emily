{
    "subject": "TRAVEL_TIME",
    "topics": [
        {
            "topic": "NONE",
            "categories": [
                {
                    "pattern": "TRAVEL * TO *",
                    "template": {
                        "type": "W",
                        "presponse": "Let me check...",
                        "command": "travel_time.main('{{1}}','{{2}}')",
                        "response": "It would take {{}} to travel from {{1}} to {{2}}"
                    }
                },
                {
                    "pattern": "HOW LONG WOULD IT TAKE TO DRIVE FROM * TO *",
                    "template": {
                        "type": "U",
                        "redirect": "TRAVEL {{1}} TO {{2}}"
                    }
                },
                {
                    "pattern": "HOW LONG WILL IT TAKE TO GET HOME FROM WORK",
                    "template": {
                        "type": "W",
                        "command": "travel_time.main('5200 Buffington Rd Atlanta, GA','204 County Cork Dr SW Mableton, GA')",
                        "response": "It will take you {{}} to get home if you leave work now"
                    }
                },
                {
                    "pattern": "HOW LONG WILL IT TAKE TO GET HOME",
                    "template": {
                        "type": "Y",
                        "var": "HOME",
                        "conditions": [
                            {
                                "pattern": "*",
                                "template": {
                                    "type": "V",
                                    "vars": [
                                        {
                                            "name": "TOPIC",
                                            "value": "LOCATION"
                                        }
                                    ],
                                    "response": "Where are you now?"
                                }
                            }
                        ],
                        "fallback": {
                            "type": "V",
                            "vars": [
                                {
                                    "name": "TOPIC",
                                    "value": "HOME"
                                }
                            ],
                            "response": "Where is home for you?"
                        }
                    }
                },
                {
                    "pattern": "HOW LONG WILL IT TAKE ME TO GET HOME",
                    "template": {
                        "type": "U",
                        "redirect": "HOW LONG WILL IT TAKE TO GET HOME"
                    }
                }
            ]
        },
        {
            "topic": "LOCATION",
            "categories": [
                {
                    "pattern": "*",
                    "template": {
                        "type": "U",
                        "vars": [
                            {
                                "name": "LOCATION",
                                "value": "{{1}}"
                            },
                            {
                                "name": "TOPIC",
                                "value": "NONE"
                            }
                        ],
                        "redirect": "TRAVEL {{LOCATION}} TO {{HOME}}"
                    }
                }
            ]
        },
        {
            "topic": "HOME",
            "categories": [
                {
                    "pattern": "*",
                    "template": {
                        "type": "V",
                        "vars": [
                            {
                                "name": "HOME",
                                "value": "{{1}}"
                            },
                            {
                                "name": "TOPIC",
                                "value": "LOCATION"
                            }
                        ],
                        "response": "Where are you now?"
                    }
                }
            ]
        }
    ]
}